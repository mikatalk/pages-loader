{
  "name": "pages-loader",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "build": "rimraf dist && webpack --config webpack-prod.config.js --bail --progress --profile && cp -R src/assets dist",
    "dev": "webpack-dev-server --config webpack-dev.config.js --watch --colors --inline --progress"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pages-loader.git"
  },
  "keywords": [
    "webpack",
    "startkit",
    "frontend",
    "es6",
    "javascript",
    "webdev"
  ],
  "author": "Mika I.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pages-loader/issues"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "rules": {
      "semi": 2,
      "libs": "off"
    }
  },
  "homepage": "https://github.com/pages-loader#readme",
  "devDependencies": {
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.2",
    "babel-preset-es2015": "^6.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.0.0",
    "eslint-loader": "^1.2.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.8.1",
    "node-sass": "^3.4.2",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {}
}
